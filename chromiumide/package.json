    "grammars": [
      {
        "language": "upstart",
        "scopeName": "source.upstart",
        "path": "./languages/upstart.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.shellscript": "shellscript"
        }
      },
      {
        "language": "portage",
        "scopeName": "source.portage",
        "path": "./languages/portage.tmLanguage.json"
      }
