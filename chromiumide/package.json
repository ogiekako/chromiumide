        "aliases": [
          "SELinux policy"
        ],
        "extensions": [
          ".te"
        ]
      },
      {
        "id": "starlark",
        "aliases": [
          "Starlark"
        ],
        "extensions": [
          ".bzl",
          ".star"
        ],
        "filenames": [
          "BUILD",
          "BUILD.bazel",
          "WORKSPACE"
        ]
      },
      {
        "id": "xml",
        "filenamePatterns": [
          "org.chromium.*.conf"
        ]
      }
